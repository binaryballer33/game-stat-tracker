{
	"firestore": {
		"database": "game-stat-tracker",
		"location": "us-east4",
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"apphosting": {
		"backendId": "game-stat-tracker",
		"rootDir": "/",
		"ignore": [
			"node_modules",
			".git",
			"firebase-debug.log",
			"firebase-debug.*.log",
			"functions"
		]
	},
	"hosting": {
		"source": ".",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"frameworksBackend": {
			"region": "us-east1"
		}
	}
}
